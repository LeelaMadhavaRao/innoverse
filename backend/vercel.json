{
  "version": 2,
  "name": "innoverse-backend",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "functions": {
    "index.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "github": {
    "enabled": true,
    "autoAlias": false,
    "autoJobCancelation": true
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}